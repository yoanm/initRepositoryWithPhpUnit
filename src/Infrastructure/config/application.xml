            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.group_inclusion" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Groups\GroupInclusionNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.group"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.groups" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\GroupsNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.group_inclusion"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.excluded_whitelist" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Filter\ExcludedWhiteListNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.whitelist_entry"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.whitelist_entry" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Filter\WhiteListEntryNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.filesystem_item"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.whitelist" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Filter\WhiteListNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.whitelist_entry"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.excluded_whitelist"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.filter" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\FilterNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.whitelist"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.log" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Logging\LogNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.logging" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\LoggingNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.log"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.listener" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Listeners\ListenerNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.listeners" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\ListenersNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.listener"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.php_item" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Php\PhpItemNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.php" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\PhpNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.php_item"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.configuration" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\ConfigurationNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.test-suites"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.groups"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.filter"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.logging"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.listeners"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.php"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.configuration_file" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\ConfigurationFileNormalizer">
