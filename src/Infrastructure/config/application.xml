<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>


        <service id="phpunit_config_manager.app.update_configuration_file_list" class="Yoanm\PhpUnitConfigManager\Application\UpdateConfigurationFileList">
            <argument type="service" id="phpunit_config_manager.infra.writer.configuration_file" />
            <argument type="service" id="phpunit_config_manager.app.updater.configuration_file" />
        </service>

        <!-- Updater -->
        <service id="phpunit_config_manager.app.updater.helper.header_footer" class="Yoanm\PhpUnitConfigManager\Application\Updater\Common\HeaderFooterHelper"/>
        <service id="phpunit_config_manager.app.updater.common.plain_value" class="Yoanm\PhpUnitConfigManager\Application\Updater\Common\PlainValueUpdater"/>
        <service id="phpunit_config_manager.app.updater.common.attribute" class="Yoanm\PhpUnitConfigManager\Application\Updater\Common\AttributeUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value" />
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.groups.item" class="Yoanm\PhpUnitConfigManager\Application\Updater\Groups\GroupItemUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value" />
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.test_suites.suite.item" class="Yoanm\PhpUnitConfigManager\Application\Updater\TestSuites\TestSuiteItemUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value" />
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.test_suites.suite" class="Yoanm\PhpUnitConfigManager\Application\Updater\TestSuites\TestSuiteUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value" />
            <argument type="service" id="phpunit_config_manager.app.updater.test_suites.suite.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.test_suites" class="Yoanm\PhpUnitConfigManager\Application\Updater\TestSuitesUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.test_suites.suite"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.groups.inclusion" class="Yoanm\PhpUnitConfigManager\Application\Updater\Groups\GroupInclusionUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.groups.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.groups" class="Yoanm\PhpUnitConfigManager\Application\Updater\GroupsUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.groups.inclusion"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.filter.whitelist.item" class="Yoanm\PhpUnitConfigManager\Application\Updater\Filter\WhiteListItemUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.filter.whitelist.excluded" class="Yoanm\PhpUnitConfigManager\Application\Updater\Filter\ExcludedWhiteListUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.filter.whitelist.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.filter.whitelist" class="Yoanm\PhpUnitConfigManager\Application\Updater\Filter\WhiteListUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.filter.whitelist.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.filter.whitelist.excluded"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.filter" class="Yoanm\PhpUnitConfigManager\Application\Updater\FilterUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.filter.whitelist"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.loging.item" class="Yoanm\PhpUnitConfigManager\Application\Updater\Logging\LogUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.logging" class="Yoanm\PhpUnitConfigManager\Application\Updater\LoggingUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.loging.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.listeners.item" class="Yoanm\PhpUnitConfigManager\Application\Updater\Listeners\ListenerUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.listeners" class="Yoanm\PhpUnitConfigManager\Application\Updater\ListenersUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.listeners.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.php.item" class="Yoanm\PhpUnitConfigManager\Application\Updater\Php\PhpItemUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.php" class="Yoanm\PhpUnitConfigManager\Application\Updater\PhpUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.php.item"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.configuration" class="Yoanm\PhpUnitConfigManager\Application\Updater\ConfigurationUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.updater.test_suites"/>
            <argument type="service" id="phpunit_config_manager.app.updater.groups"/>
            <argument type="service" id="phpunit_config_manager.app.updater.filter"/>
            <argument type="service" id="phpunit_config_manager.app.updater.logging"/>
            <argument type="service" id="phpunit_config_manager.app.updater.listeners"/>
            <argument type="service" id="phpunit_config_manager.app.updater.php"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>
        <service id="phpunit_config_manager.app.updater.configuration_file" class="Yoanm\PhpUnitConfigManager\Application\Updater\ConfigurationFileUpdater">
            <argument type="service" id="phpunit_config_manager.app.updater.common.plain_value"/>
            <argument type="service" id="phpunit_config_manager.app.updater.configuration"/>
            <argument type="service" id="phpunit_config_manager.app.updater.helper.header_footer" />
        </service>

        <!-- Normalizers -->
        <service id="phpunit_config_manager.app.serializer.normalizer.attribute" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Common\AttributeNormalizer"/>
        <service id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Common\UnmanagedNodeNormalizer"/>
        <service id="phpunit_config_manager.app.serializer.normalizer.group" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Groups\GroupNormalizer"/>
        <service id="phpunit_config_manager.app.serializer.normalizer.filesystem_item" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\Common\FilesystemItemNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.test_suite_item" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\TestSuites\TestSuite\TestSuiteItemNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.filesystem_item"/>
        </service>
        <service id="phpunit_config_manager.app.serializer.normalizer.test-suite" class="Yoanm\PhpUnitConfigManager\Application\Serializer\Normalizer\TestSuites\TestSuiteNormalizer">
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.attribute"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.test_suite_item"/>
            <argument type="service" id="phpunit_config_manager.app.serializer.normalizer.unmanaged_node"/>
